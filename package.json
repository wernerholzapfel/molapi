{
  "name": "molapi",
  "version": "1.0.0",
  "description": "Mol API",
  "scripts": {
    "typings": "typings install",
    "build": "tsc",
    "dev": "nodemon",
    "prestart:prod": "tsc",
    "prestart": "npm run build",
    "prepublish": "npm run build",
    "start": "node dist/server.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wernerholzapfel/molapi.git"
  },
  "keywords": [],
  "author": "Werner Holzapfel <werner.holzapfel@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/wernerholzapfel/molapi/issues"
  },
  "homepage": "https://github.com/wernerholzapfel/molapi#readme",
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/microservices": "^5.4.0",
    "@nestjs/testing": "^5.4.0",
    "@nestjs/websockets": "^5.4.0",
    "@types/async": "^2.0.50",
    "@types/auth0": "^2.9.5",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-jwt": "0.0.40",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.12.2",
    "@types/node-cache": "^4.1.1",
    "@types/slug": "^0.9.0",
    "async": "^2.6.1",
    "auth0": "^2.13.0",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "dotenv": "^6.1.0",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.3.0",
    "jwt-decode": "^2.2.0",
    "node-cache": "^4.2.0",
    "pg": "^7.6.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "slug": "^0.9.2",
    "ts-node": "^7.0.1",
    "typeorm": "^0.2.8",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "nodemon": "^1.18.5",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec.(ts|tsx|js)$"
  }
}

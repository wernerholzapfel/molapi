{
  "name": "molapi",
  "version": "1.0.0",
  "description": "Mol API",
  "scripts": {
    "typings": "typings install",
    "build": "tsc",
    "dev": "nodemon",
    "prestart:prod": "tsc",
    "prestart": "npm run build",
    "prepublish": "npm run build",
    "start": "node dist/server.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wernerholzapfel/molapi.git"
  },
  "keywords": [],
  "author": "Werner Holzapfel <werner.holzapfel@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/wernerholzapfel/molapi/issues"
  },
  "homepage": "https://github.com/wernerholzapfel/molapi#readme",
  "dependencies": {
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "@nestjs/microservices": "^5.7.4",
    "@nestjs/testing": "^5.7.4",
    "@nestjs/typeorm": "^5.3.0",
    "@nestjs/websockets": "^5.7.4",
    "@types/async": "^2.4.1",
    "@types/auth0": "^2.9.10",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.41",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^11.11.1",
    "@types/node-cache": "^4.1.3",
    "@types/slug": "^0.9.1",
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "express-jwt": "^5.3.1",
    "firebase-admin": "^7.0.0",
    "jwks-rsa": "^1.4.0",
    "jwt-decode": "^2.2.0",
    "node-cache": "^4.2.0",
    "pg": "^7.8.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "slug": "^1.0.0",
    "ts-node": "^8.0.3",
    "typeorm": "^0.2.15-rc.2",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.4.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec.(ts|tsx|js)$"
  }
}
